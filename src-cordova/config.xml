<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="NaN" id="com.bigjump.preflighttest" version="1.0.19" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pre Flight Test</name>
    <description>A pre flight test application</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Tom
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
    </platform>
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="ios" spec="^4.5.4" />
</widget>
